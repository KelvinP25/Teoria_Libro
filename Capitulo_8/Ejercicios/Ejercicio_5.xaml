<Window x:Class="Capitulo_8.Ejercicios.Ejercicio_5"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Capitulo_8.Ejercicios"
        mc:Ignorable="d"
        Title="Ejercicio_5" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <StackPanel>
                <Label Content="Ingrese una primera cadena: " Margin="5,5,0,0"/>
                <TextBox x:Name="PrimeraCadenaTextBox" Margin="10,0"/>
                
                <Label Content="Ingrese una Segunda cadena: " Margin="5,5,0,0"/>
                <TextBox x:Name="SegundaCadenaTextBox" Margin="10,0"/>

                <Button x:Name="OrdenarButton" Content="Ordenar cadenas" Width="120" Click="OrdenarButton_Click"/>
            </StackPanel>
        </StackPanel>
    </Grid>
</Window>
